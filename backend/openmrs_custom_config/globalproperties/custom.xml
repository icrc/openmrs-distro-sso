<config>
  <globalProperties>
    <!--    Oauth2 -->
    <globalProperty>
      <property>oauth2login.redirectUriAfterLogin</property>
      <value>/spa/home</value>
    </globalProperty>
<!--    Datafilter-->
    <globalProperty>
      <property>datafilter.patientLocationLinkingInterceptor.enabled</property>
      <value>true</value>
      <description>
        Enables Data Filter's PatientLocationLinkingInterceptor that associates newly created patients with the current
        user's session location.
      </description>
    </globalProperty>
<!--    ID link for location prefix-->
    <globalProperty>
      <property>idgen.prefixLocationAttributeType</property>
      <value>Code</value>
    </globalProperty>
  </globalProperties>
</config>